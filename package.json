{
  "name": "poke-api-testing",
  "version": "1.0.0",
  "description": "Automated tests for PokeAPI.co",
  "main": "",
  "scripts": {
    "open": "npm run lint-fix && cypress open",
    "test": "npm run lint-fix && cypress run",
    "lint": "npx eslint \"**/integration/**/*.js\"",
    "lint-fix": "npx eslint --fix \"**/integration/**/*.js\""
  },
  "eslintIgnore": [
    "artifacts/**/*.js"
  ],
  "license": "MIT",
  "dependencies": {
    "@testing-library/cypress": "8.0.2",
    "cypress": "9.6.1",
    "cypress-commands": "2.0.1",
    "cypress-plugin-retries": "1.5.2",
    "eslint": "8.15.0",
    "eslint-plugin-cypress": "2.12.1",
    "prettier": "2.6.2"
  }
}
